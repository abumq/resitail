{
    "residue_config" : "<residue-config>",
    "hooks": [
        {
            "name": "console",
            "path": "./hooks/console",
            "enabled": false,
            "config": {
                "channels" : {
                    "to_client" : true,
                    "to_logger" : true
                }
            }
        },
        {
            "name": "browser",
            "package": "resitail-browser",
            "version": "latest",
            "enabled": true,
            "config": {
                "port": 3000,
                "channels" : {
                    "to_client" : true,
                    "to_logger" : false
                }
            }
        }
    ]
}
